#include<iostream>
using namespace std;

int main()
{
    int num;
	cout<<"Enter the number\n";
	cin>>num;
	
	int first = 0;
	int second = 1;
	int next = 1;
	
	cout<<"The Fibonacci series is: ";
	cout<<first<<" "<<second<<" ";
	while(next<=num)
	{
		cout<<next<<" ";
		
		first = second;
		second = next;
		
		next = first + second;
	
	}	
	
	return 0;
}
